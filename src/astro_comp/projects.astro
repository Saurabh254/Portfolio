<main
  class="w-full bg-secondary flex flex-col justify-center items-center p-4 mb-24"
  style="height: fit-content;"
  id="projects"
>
  <div
    class="max-w-[100%] flex flex-col font-semibold w-full h-[80dvh] mt-12 md:mt-24"
  >
    <h1 class="text-xl border-b-2 border-gray-200 pb-4 md:pb-4 font-semibold">
      Projects
    </h1>
    <div class="w-full flex flex-col gap-8 mt-4 md:mt-8">
